<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/rectangle_radius"
        tools:ignore="UnusedAttribute">
        <shape android:shape="rectangle" android:tint="?colorAccent">
            <corners android:topRightRadius="0dp"
                android:bottomRightRadius="0dp"/>
            <solid android:color="@color/light_translucent"/>
        </shape>
    </item>

    <item android:id="@+id/rectangle"
        android:width="5dp"
        tools:ignore="UnusedAttribute">
        <shape android:tint="?colorAccent" android:shape="rectangle">
            <solid android:color="#000" />
        </shape>
    </item>

</layer-list>