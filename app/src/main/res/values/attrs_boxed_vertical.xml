<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BoxedVertical">
        <attr name="boxed_points" format="float" />
        <attr name="boxed_max" format="float" />
        <attr name="boxed_min" format="float"/>
        <attr name="boxed_defaultValue" format="integer" />
        <attr name="boxed_textBottomPadding" format="integer" />
        <attr name="boxed_libCornerRadius" format="integer" />
        <attr name="boxed_step" format="float"/>

        <attr name="boxed_defaultImage" format="reference"/>
        <attr name="boxed_minImage" format="reference"/>
        <attr name="boxed_maxImage" format="reference"/>

        <attr name="boxed_progressColor" format="color" />
        <attr name="boxed_backgroundColor" format="color" />

        <attr name="boxed_textSize" format="dimension"/>
        <attr name="boxed_textColor" format="color"/>

        <attr name="boxed_clockwise" format="boolean" />
        <attr name="boxed_touchDisabled" format="boolean" />
        <attr name="boxed_textEnabled" format="boolean" />
        <attr name="boxed_imageEnabled" format="boolean" />

    </declare-styleable>

    <color name="boxed_color_text">@android:color/black</color>
    <color name="boxed_color_progress">#fbfbfb</color>
    <color name="boxed_color_background">#4b4b4b</color>

</resources>